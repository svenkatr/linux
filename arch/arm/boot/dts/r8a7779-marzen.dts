/dts-v1/;
#include "r8a7779.dtsi"

/ {
	model = "marzen";
	compatible = "renesas,marzen", "renesas,r8a7779";

	chosen {
		bootargs = "console=ttySC2,115200 earlyprintk=sh-sci.2,115200 ignore_loglevel root=/dev/nfs ip=on";
	};

	memory {
		device_type = "memory";
		reg = <0x60000000 0x40000000>;
	};
};
